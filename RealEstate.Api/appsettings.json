{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithCorrelationId"
    ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog"
        }
      }
    ]
  },
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:5173",
      "http://127.0.0.1:5173",
      "http://localhost:8080",
      "http://127.0.0.1:8080"
    ]
  },
  "Mongo": {
    "ConnectionString": "mongodb://realestate_app:123456@localhost:27017/realestate?authSource=realestate",
    "Database": "realestate",
    "OwnersCollection": "Owners",
    "PropertiesCollection": "Properties",
    "PropertyImagesCollection": "PropertyImages",
    "PropertyTracesCollection": "PropertyTraces"
  }
}
